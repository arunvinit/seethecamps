<% layout('layouts/boilerplate')%>
<h2>All campgrounds</h2>
    <ul>
        <% for(let campground of campgrounds){%>
            <div class="card" mb-3>
                <div class="row">
                    <div class="col-md-4">
                        <% if(campground.images.length) { %> 
                            <img class="img-fluid" src="<%=campground.images[0].url%>" alt="" height="300px">
                        <% }else{ %>
                            <img class="img-fluid" src="data:image/jpeg;base64," alt="" height="auto" width="100%"> 
                        <% } %> 
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title"><%=campground.title%></h5>
                            <p class="card-text"><%=campground.description%></p>
                            <a href="/campground/<%=campground._id%>" class="btn btn-primary">See this Campground</a>
                        </div>
                    </div>
                </div>
            </div>
        <% } %> 
    </ul>
    <a href="/campground/new">Add New Campground</a>

